#version 330 core

out vec4 FragColor;

in vec2 TexCoords;
in vec3 FragPos;
in vec3 Normal;
in vec3 WorldNormal;
in vec3 WorldPos;
in vec4 FragPosLightSpace;

uniform sampler2D textureDiffuse;

void main()
{    
    vec3 color = texture(textureDiffuse, TexCoords).rgb;

    FragColor = vec4(color, 1.0);
}